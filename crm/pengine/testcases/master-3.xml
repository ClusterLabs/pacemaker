<?xml version="1.0"?>
<!DOCTYPE cib SYSTEM "crm-1.0.dtd">
<cib num_updates="1" epoch="1" admin_epoch="0">
  <configuration>
    <crm_config><cluster_property_set id="cib-bootstrap-options"><attributes>
      <nvpair id="no_quorum_policy" name="no_quorum_policy" value="ignore"/>
    </attributes></cluster_property_set></crm_config>
    <nodes>
      <node id="uuid1" uname="node1" type="member">
        <instance_attributes id="master-uuid1">
          <attributes>
            <nvpair id="master-child_rsc1:0-uuid1" name="master-child_rsc1:0" value="300"/>
            <nvpair id="master-child_rsc1:1-uuid1" name="master-child_rsc1:1" value="100"/>
            <nvpair id="master-child_rsc1:2-uuid1" name="master-child_rsc1:2" value="400"/>
            <nvpair id="master-child_rsc1:3-uuid1" name="master-child_rsc1:3" value="40"/>
            <nvpair id="master-child_rsc1:4-uuid1" name="master-child_rsc1:4" value="50"/>
          </attributes>
        </instance_attributes>
      </node>
      <node id="uuid2" uname="node2" type="member">
        <instance_attributes id="master-uuid2">
          <attributes>
            <nvpair id="master-child_rsc1:0-uuid2" name="master-child_rsc1:0" value="10"/>
            <nvpair id="master-child_rsc1:1-uuid2" name="master-child_rsc1:1" value="30"/>
            <nvpair id="master-child_rsc1:2-uuid2" name="master-child_rsc1:2" value="20"/>
            <nvpair id="master-child_rsc1:3-uuid2" name="master-child_rsc1:3" value="200"/>
            <nvpair id="master-child_rsc1:4-uuid2" name="master-child_rsc1:4" value="60"/>
          </attributes>
        </instance_attributes>
      </node>
    </nodes>
    <resources>
      <master_slave id="rsc1">
        <instance_attributes>
          <attributes>
            <nvpair id="rsc1-attr1" name="clone_max" value="5"/>
            <nvpair id="rsc1-attr2" name="clone_node_max" value="2"/>
            <nvpair id="rsc1-attr3" name="master_max" value="2"/>
            <nvpair id="rsc1-attr4" name="master_node_max" value="1"/>
            <nvpair name="resource_stickiness" value="1"/>
          </attributes>
        </instance_attributes>
	<primitive id="child_rsc1" class="heartbeat" type="apache"/>
      </master_slave>
    </resources>
    <constraints>
      <rsc_location id="rsc1_master" rsc="rsc1">
	<rule id="pref_rsc1_master-1" score="100" role="Master">
          <expression attribute="#uname" operation="eq" value="node2"/>
        </rule>
	<rule id="pref_rsc1_master-2" score="-INFINITY" role="Master">
          <expression attribute="#uname" operation="eq" value="node1"/>
        </rule>
      </rsc_location>
    </constraints>
  </configuration>
  <status>
    <node_state id="uuid1" ha="active" uname="node1" crmd="online" join="member" expected="member" in_ccm="true">
      <transient_attributes>
        <instance_attributes>
	  <attributes>
	    <nvpair id="probed" name="probe_complete" value="true"/>
	  </attributes>
        </instance_attributes>
      </transient_attributes>
    </node_state>
    <node_state id="uuid2" ha="active" uname="node2" crmd="online" join="member" expected="member" in_ccm="true">
      <transient_attributes>
        <instance_attributes>
	  <attributes>
	    <nvpair id="probed" name="probe_complete" value="true"/>
	  </attributes>
        </instance_attributes>
      </transient_attributes>
    </node_state>
  </status>
</cib>
