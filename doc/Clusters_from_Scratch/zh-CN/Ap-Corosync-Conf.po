#~ "totem {\n"
#~ "        version: 2\n"
#~ "\n"
#~ "        # How long before declaring a token lost (ms)\n"
#~ "        token:          5000\n"
#~ "\n"
#~ "        # How many token retransmits before forming a new configuration\n"
#~ "        token_retransmits_before_loss_const: 10\n"
#~ "\n"
